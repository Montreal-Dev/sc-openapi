{
  "type": "module",
  "author": "Rabel",
  "name": "sc-openapi",
  "description": "A fully typed, auto-generated TypeScript client for the SoundCloud API, built from OpenAPI using Hey-API.",
  "version": "1.0.27",
  "input": "https://raw.githubusercontent.com/swagger-api/swagger-petstore/master/src/main/resources/openapi.yaml",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "gen": "openapi-ts",
    "sync:patch": "node lib/scripts/sync-patch.js",
    "sync:version": "node lib/scripts/sync-version.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "pnpm gen && pnpm build"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.87.5",
    "eslint": "^9.39.1",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "yaml": "^2.8.1"
  }
}
